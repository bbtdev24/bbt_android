<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#0F4C81" />
            <size
                android:width="25dp"
                android:height="25dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="5dp"
                android:color="@android:color/transparent" />
            <solid android:color="#fff" />
            <size
                android:width="45dp"
                android:height="45dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="10dp"
                android:color="@android:color/transparent" />
            <solid android:color="#0F4C81" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>
</layer-list>


